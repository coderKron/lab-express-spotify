<div>

  {{#each arrOfTracks}}
    <h2>{{this.name}}</h2>
    <figure>
      <figcaption>Listen to: {{this.name}}</figcaption>
      <audio controls src="{{this.preview_url}}">
        Your browser does not support the
        <code>audio</code>
        element.
      </audio>
    </figure>
  {{/each}}

</div>